            body = body + "\n Format Telephone invalide: " + numTel;

            MailMessage msg = new MailMessage(sender, recever);
            msg.Body = body;
            msg.Subject = "Rapport chargement employe";
            SmtpClient smtpclient = new SmtpClient(smtp,587);
            smtpclient.EnableSsl = true;
            smtpclient.UseDefaultCredentials = false;
            System.Net.NetworkCredential cred = new System.Net.NetworkCredential(sender,"domnik");
            smtpclient.UseDefaultCredentials = false;
            smtpclient.Credentials = cred;
            smtpclient.Send(msg);

