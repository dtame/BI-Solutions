            String recever = "d_tamo@yahoo.fr";//"yazid.grim@montrealcollege.ca";
            String smtp = "smtp.gmail.com";
            String body = "Bonjour, \n rapport de chargement de la source de donnees employes: \n ";

            int numDefault = (int)Dts.Variables["User::NumberDefault"].Value;
            int numAdrnull = (int)Dts.Variables["User::NumberAdrNull"].Value;
            int numEmailnull = (int)Dts.Variables["User::NumberEmailNull"].Value;
